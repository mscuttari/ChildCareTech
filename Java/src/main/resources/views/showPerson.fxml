<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>

<AnchorPane fx:id="showPersonPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.client.controllers.ShowPersonController">
   <children>
      <TableView fx:id="tablePeople" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="70.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <columns>
            <TableColumn fx:id="columnPeopleFirstName" style="-fx-alignment: center" text="Nome" />
            <TableColumn fx:id="columnPeopleLastName" style="-fx-alignment: center" text="Cognome" />
            <TableColumn fx:id="columnPeopleFiscalCode" style="-fx-alignment: center" text="Codice fiscale" />
            <TableColumn fx:id="columnPeopleType" style="-fx-alignment: center" text="Tipo" />
            <TableColumn fx:id="columnPeopleEdit" style="-fx-alignment: center" />
            <TableColumn fx:id="columnPeopleDelete" style="-fx-alignment: center" />
         </columns>
      </TableView>
      <ImageView fx:id="goBackImage" fitHeight="60.0" fitWidth="60.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="30.0">
         <image>
            <Image url="@../images/goBack(blue).png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
